it Promise.all(requests)\r\n\r\n    weatherData.forEach((value, index) => {\r\n      saveCities.value[index].weather = value.data;\r\n    });\r\n  }\r\n};\r\nawait getCities()\r\nconst router = useRouter()\r\nconst geToCityView = (city) =>{\r\n  router.push({\r\n    name: \"cityView\",\r\n    params: { state: city.state.replace(\" \",\"\"), city: city.replace(\" \",\"\") },\r\n    query: {\r\n      lat: city.coords.lat,\r\n      lng: city.coords.lng,\r\n      preview: true\r\n    }\r\n  })\r\n}\r\n</script>"],"mappings":";;2BACEA,mBAAA,CAEMC,SAAA,QAHRC,WAAA,CACsBC,MAAA,CAAAC,UAAU,EAAlBC,IAAI;yBAAhBL,mBAAA,CAEM;MAF2BM,GAAG,EAAED,IAAI,CAACE;QACzCC,YAAA,CAAoDL,MAAA;MAAzCE,IAAI,EAAEA,IAAI;MAAGI,OAAK,EAAAC,MAAA,IAAEP,MAAA,CAAAQ,YAAY,CAACN,IAAI","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}