timezone_offset;\r\n\r\n    // call hourly weather offset\r\n    weatherData.data.hourly.forEach((hour) => {\r\n      const utc = hour.dy * 1000 + localOffset;\r\n      hour.currentTime = utc + 100 * weatherData.data.timezone_offset;\r\n    });\r\n    return weatherData;\r\n  } catch (err) {\r\n    console.log(err);\r\n  }\r\n};\r\nconst weatherData = await getWeatherData();\r\nconsole.log(weatherData);\r\n</script>"],"mappings":";;EAAAA,GAAA;EAIIC,KAAK,EAAC;;;uBAJVC,mBAAA,CAAAC,SAAA,SACAC,mBAAA,YAAe,EAELC,MAAA,CAAAC,KAAK,CAACC,KAAK,CAACC,OAAO,I,cAD3BN,mBAAA,CAKM,OALNO,UAKM,EAFL,6FAED,KAPFL,mBAAA,gBAQAA,mBAAA,sBAAyB,C","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}